{
	"name": "phoxphp/phoxphp",
	"description": "PhoxPHP Framework Package",
	"type":"project",
	"keywords": ["php", "php7", "framework"],
	"homepage": "https://phoxphp.com",
	"author": [
		{
			"name":"Peter Taiwo",
			"email":"peter@phoxphp.com"
		}
	],
	"autoload": {
		"psr-4": {
			"App\\": "app/app"
		}
	},
	"config": {
		"vendor-dir": "app/vendor",
		"sort-packages": false,
		"optimize-autoloader": true
	},
	"require": {
		"phoxphp/cache": ">=1.0.2",
		"phoxphp/glider": ">=1.3.2",
		"phoxphp/filesystem": ">=1.2.1",
		"phoxphp/http": ">=1.5.1",
		"phoxphp/translation": ">=1.0.3",
		"phoxphp/view": ">=1.0.0",
		"phoxphp/dependency-injection": ">=1.1.0",
		"phoxphp/log": ">=1.0.2",
		"phoxphp/prop": ">=1.0.0",
		"phoxphp/auth": ">=1.0.0",
		"phoxphp/validator": ">=1.0.1",
		"phoxphp/phoxengine": ">=1.0.0",
		"php": ">=7.0.1"
	},
	"scripts": {
		"post-create-project-cmd": [
			"Kit\\Glider\\Composer::postInstall",
			"Kit\\Auth\\Configurator::fixConfiguration",
			"Kit\\PhoxEngine\\Configurator::fixConfiguration",
			"Kit\\Translation\\Configurator::fixConfiguration",
			"Kit\\Validator\\Configurator::fixConfiguration"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}