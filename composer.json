{
	"name": "phoxphp/phoxphp",
	"description": "PhoxPHP Framework Package",
	"type":"project",
	"keywords": ["php", "php7", "framework"],
	"homepage": "https://phoxphp.com",
	"author": [
		{
			"name":"Peter Taiwo",
			"email":"taiwop3@gmail.com",
			"role":"creator"
		}
	],
	"autoload": {
		"psr-4": {
			"App\\": "app/app"
		}
	},
	"config": {
		"vendor-dir": "app/vendor",
		"sort-packages": false,
		"optimize-autoloader": true
	},
	"require": {
		"phoxphp/cache": ">=1.0.2",
		"phoxphp/glider": ">=1.0.2",
		"phoxphp/filesystem": "^1.0.1",
		"phoxphp/http": "1.0.3",
		"phoxphp/translation": "1.0.3",
		"phoxphp/view": ">=1.0.0",
		"phoxphp/dependency-injection": "~1.0.0",
		"phoxphp/log": "dev-master",
		"phoxphp/auth": "dev-develop",
		"php": ">=7.0.1"
	},
	"scripts": {
		"post-install-cmd": [
			"Kit\\Glider\\Configurator::fixConfiguration",
			"Kit\\Auth\\Configurator::fixConfiguration"
		],
		"post-update-cmd": [
			"Kit\\Glider\\Configurator::fixConfiguration",
			"Kit\\Auth\\Configurator::fixConfiguration"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}